{
  "expo": {
    "name": "UpdatesAPIDemo",
    "slug": "UpdatesAPIDemo",
    "scheme": "updatesapidemo",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-all.png",
    "splash": {
      "image": "./assets/images/splash-logo-all.png",
      "resizeMode": "contain",
      "backgroundColor": "#191015"
    },
    "updates": {
      "requestHeaders": {
        "expo-channel-name": "main"
      },
      "url": "https://u.expo.dev/a3cd8299-57f8-44b4-9ae7-0548097a2432"
    },
    "jsEngine": "hermes",
    "android": {
      "icon": "./assets/images/app-icon-android-legacy.png",
      "package": "com.updatesapidemo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-android-adaptive-foreground.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "./assets/images/splash-logo-android-universal.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "ios": {
      "icon": "./assets/images/app-icon-ios.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.updatesapidemo",
      "splash": {
        "image": "./assets/images/splash-logo-ios-mobile.png",
        "tabletImage": "./assets/images/splash-logo-ios-tablet.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "extra": {
      "criticalIndex": 1,
      "message": "Automatic update",
      "updates": {
        "assetPatternsToBeBundled": [
          "app/includedImages/*",
          "app/includedImages/*"
        ]
      },
      "eas": {
        "projectId": "a3cd8299-57f8-44b4-9ae7-0548097a2432"
      }
    },
    "plugins": [
      "expo-localization",
      "expo-build-properties",
      "expo-font"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "owner": "hterauchi"
  }
}